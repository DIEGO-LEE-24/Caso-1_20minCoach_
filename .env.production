# Production Environment Configuration
NODE_ENV=production
VITE_APP_ENV=production

# Supabase Configuration
VITE_SUPABASE_URL=https://prod-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_prod_anon_key_here

# API Configuration
VITE_API_URL=https://api.20mincoach.app
VITE_API_TIMEOUT=15000

# Feature Flags
VITE_ENABLE_LOGGING=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_2FA=true

# Development Tools
VITE_SHOW_DEV_TOOLS=false
VITE_HOT_RELOAD=false

# WebRTC Configuration
VITE_WEBRTC_ICE_SERVERS=["stun:stun.l.google.com:19302", "turn:prod-turn.20mincoach.app:443"]

# Cache Settings
VITE_CACHE_TTL=3600

# Monitoring
VITE_SENTRY_DSN=your_production_sentry_dsn
VITE_ANALYTICS_ID=your_production_analytics_id

# Security
VITE_SECURITY_HEADERS=true
VITE_CSP_ENABLED=true

# Performance
VITE_LAZY_LOADING=true
VITE_BUNDLE_ANALYZER=false