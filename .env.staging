# Staging Environment Configuration
NODE_ENV=staging
VITE_APP_ENV=staging

# Supabase Configuration
VITE_SUPABASE_URL=https://staging-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_staging_anon_key_here

# API Configuration
VITE_API_URL=https://staging-api.20mincoach.app
VITE_API_TIMEOUT=10000

# Feature Flags
VITE_ENABLE_LOGGING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_2FA=true

# Development Tools
VITE_SHOW_DEV_TOOLS=false
VITE_HOT_RELOAD=false

# WebRTC Configuration
VITE_WEBRTC_ICE_SERVERS=["stun:stun.l.google.com:19302", "turn:staging-turn.20mincoach.app"]

# Cache Settings
VITE_CACHE_TTL=600

# Monitoring
VITE_SENTRY_DSN=your_staging_sentry_dsn
VITE_ANALYTICS_ID=your_staging_analytics_id